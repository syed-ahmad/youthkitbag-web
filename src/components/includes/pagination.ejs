<nav aria-label="Page navigation">
    <ul class="pagination">
        <li class="page-item <% if (currentPage == 1) { %>disabled<% } %>"><a class="page-link" href="?page=1<%= incSearch %>">First</a></li>
        <li class="page-item <% if (!hasPreviousPage) { %>disabled<% } %>"><a class="page-link" href="?page=<%= previousPage %><%= incSearch %>">Previous</a></li>
        <li class="page-item active"><a class="page-link" href="?page=<%= currentPage %><%= incSearch %>"><%= currentPage %></a></li>
        <li class="page-item <% if (!hasNextPage) { %>disabled<% } %>"><a class="page-link" href="?page=<%= nextPage %><%= incSearch %>">Next</a></li>
        <li class="page-item <% if (lastPage == currentPage) { %>disabled<% } %>"><a class="page-link" href="?page=<%= lastPage %><%= incSearch %>">Last</a></li>
    </ul>
</nav>